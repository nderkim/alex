    "@types/node": "^14.14.33",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/webpack-env": "^1.16.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.0",
    "eslint": "^7.20.0",
