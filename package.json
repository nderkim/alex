{
  "name": "alex",
  "version": "0.0.0",
  "bin": "./bin.js",
  "scripts": {
    "start": "node scripts/start",
    "build": "node scripts/build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "npm run format && npm run lint && npm run tsc",
    "tsc": "npm run tsc:browser && npm run tsc:client && npm run tsc:common && npm run tsc:node && npm run tsc:server",
    "tsc:browser": "tsc --noEmit -p lib/browser",
    "tsc:client": "tsc --noEmit -p src/client",
    "tsc:common": "tsc --noEmit -p lib/common",
    "tsc:node": "tsc --noEmit -p lib/node",
    "tsc:server": "tsc --noEmit -p src/server",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "clean": "node scripts/clean"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
